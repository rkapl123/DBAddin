<DBsheetConfig>
<connID>pubs</connID>
<table>pubs.dbo.authors</table>
<query>SELECT T1.au_id, T1.au_lname, T1.au_fname, T1.phone, T1.address, T1.city, T1.state, T1.zip, T1.contract
FROM pubs.dbo.authors T1 
</query>
<whereClause></whereClause>
<primcols>1</primcols>
<columns>
<field><name>*au_id</name><primkey>True</primkey></field>
<field><name>*au_lname</name></field>
<field><name>*au_fname</name></field>
<field><name>*phone</name></field>
<field><name>address</name></field>
<field><name>city</name></field>
<field><name>state</name><lookup>CA||IN||KS||MD||MI||OR||TN||UT</lookup></field>
<field><name>zip</name></field>
<field><name>*contract</name></field>
</columns>
</DBsheetConfig>
